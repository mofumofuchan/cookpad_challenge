<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.user.email %>
      | content:
      <%= article.content %>
      | created_at:
      <%= article.created_at %>
      | image:
      <%= image_tag article.drawing.url if article.drawing? %>
    </li>
  <% end %>
</ul>
